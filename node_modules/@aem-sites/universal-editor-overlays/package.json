{
  "name": "@aem-sites/universal-editor-overlays",
  "version": "0.0.10",
  "description": "A collection of utils related to Overlays in Universal Editor.",
  "keywords": [
    "universal-editor"
  ],
  "author": "editorxp@adobe.com",
  "homepage": "https://git.corp.adobe.com/CQ/universal-editor",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://artifactory.corp.adobe.com/artifactory/api/npm/npm-aem-sites-release/"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "src": "src",
    "test": "tests"
  },
  "files": [
    "readme.md",
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "git@git.corp.adobe.com:CQ/universal-editor-utils.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prepack": "yarn clean && yarn lint && yarn build && yarn build:ts:declarations",
    "build": "babel src/ -d dist/ --source-maps --extensions .ts,.tsx",
    "test": "NODE_ENV=test jest --coverage",
    "lint": "eslint src tests",
    "lint:fix": "yarn lint --fix",
    "postlint": "yarn check:types",
    "build:ts:declarations": "tsc --declaration --emitDeclarationOnly -p tsconfig.build.json",
    "check:types": "tsc -p . --noEmit",
    "push": "yarn prepack && yalc push"
  },
  "pre-commit": [
    "lint"
  ],
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "debounce": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.7.4",
    "use-debounce": "^8.0.3"
  },
  "devDependencies": {
    "@aem-sites/universal-editor-config": "^0.0.8",
    "@babel/core": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@types/react-dom": "^18.0.6",
    "eslint": "^8.21.0",
    "lint-staged": "^13.0.3",
    "rimraf": "^3.0.2"
  }
}
